    if len(intersections) > 0:
        # Plot all intersections
        ax.scatter(intersections[:, 0], intersections[:, 1],
                  c=color, alpha=0.6, s=20, label='Intersections')
        
        # Connect consecutive points to show map
        if len(intersections) > 1:
            for j in range(len(intersections)-1):
                ax.plot([intersections[j, 0], intersections[j+1, 0]],
                       [intersections[j, 1], intersections[j+1, 1]],
                       'k-', alpha=0.3, linewidth=0.5)
        
        # Plot identity line (fixed points would lie here)
        x_lims = ax.get_xlim()
        y_lims = ax.get_ylim()
        lim_min = min(x_lims[0], y_lims[0])
        lim_max = max(x_lims[1], y_lims[1])
        ax.plot([lim_min, lim_max], [lim_min, lim_max], 
                'k--', alpha=0.3, label='Identity')
        
        ax.set_xlabel('First Component')
        ax.set_ylabel('Second Component')
        ax.set_title(f'Poincar√© Map: {title}')
        ax.legend(fontsize=8)
        ax.grid(True, alpha=0.3)
        ax.set_aspect('equal')

# Plot 5: Return map analysis (1D)
ax = axes[1, 2]
plane = 'x=0'
intersections = poincare_maps[plane]

if len(intersections) > 1:
    # Use bank component for 1D map
    bank_values = intersections[:, 0]
    
    # Plot return map: x_n vs x_{n+1}
    ax.scatter(bank_values[:-1], bank_values[1:],
              c='purple', alpha=0.6, s=30, label='Returns')
    
    # Plot identity line
    x_lims = ax.get_xlim()
    y_lims = ax.get_ylim()
    lim_min = min(x_lims[0], y_lims[0])
    lim_max = max(x_lims[1], y_lims[1])
    ax.plot([lim_min, lim_max], [lim_min, lim_max], 
            'k--', alpha=0.3, label='Identity')
    
    # Fit polynomial to see functional relationship
    if len(bank_values) > 10:
        coeffs = np.polyfit(bank_values[:-1], bank_values[1:], 3)
        poly = np.poly1d(coeffs)
        x_fit = np.linspace(lim_min, lim_max, 100)
        ax.plot(x_fit, poly(x_fit), 'r-', alpha=0.7, 
                linewidth=2, label='Cubic Fit')
    
    ax.set_xlabel('Bank at intersection n')
    ax.set_ylabel('Bank at intersection n+1')
    ax.set_title('1D Return Map (Bank Component)')
