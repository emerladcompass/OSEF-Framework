titles = ['Pitch=0 Section', 'Bank=0 Section', 'Power=0.5 Section']
colors = ['red', 'green', 'blue']

for i, (plane, title, color) in enumerate(zip(planes, titles, colors)):
    row = (i + 1) // 3
    col = (i + 1) % 3
    ax = axes[row, col]
    
