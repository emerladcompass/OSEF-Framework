# Cell 16: Performance Comparison
print("\n‚ö° Comparing computation methods...")

# Define different computation methods
methods = ['euler', 'rk2', 'rk4', 'adaptive']

performance_data = []

for method in methods:
    print(f"  Testing {method} method...")
    
    # Time the computation
    import time
    
    # Create model with specific method
    model = LimitCycleModel(mu=2.0, epsilon=0.1, dt=0.01, method=method)
    
    # Run multiple times for accurate timing
    times = []
    for _ in range(10):
        start_time = time.perf_counter()
        result = model.compute_limit_cycle(max_iter=2000)
        end_time = time.perf_counter()
        times.append((end_time - start_time) * 1000)  # Convert to ms
    
    avg_time = np.mean(times)
    std_time = np.std(times)
    
    # Check accuracy (period compared to reference)
    reference_period = 6.663  # Reference value from high-accuracy computation
    
    performance_data.append({
        'method': method,
        'avg_time_ms': avg_time,
        'std_time_ms': std_time,
        'period': result['period'],
        'error_percent': 100 * abs(result['period'] - reference_period) / reference_period,
        'converged': result['converged']
    })
    
    print(f"    Avg time: {avg_time:.2f} ¬± {std_time:.2f} ms")
    print(f"    Period: {result['period']:.3f}s "
          f"(Error: {performance_data[-1]['error_percent']:.1f}%)")

perf_df = pd.DataFrame(performance_data)
print(f"\n‚úÖ Performance comparison complete!")
```

