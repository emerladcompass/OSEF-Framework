for i, (key, cycle) in enumerate(limit_cycles.items()):
    points = cycle['points']
    time = np.arange(len(points)) * advanced_model.dt
    amplitude = np.sqrt(points[:, 0]**2 + points[:, 1]**2)
    ax.plot(time[:500], amplitude[:500], 
            color=colors[i], alpha=0.7, linewidth=1.5,
            label=f'Cycle {i+1}')

