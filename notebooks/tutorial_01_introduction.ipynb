# Cell 12: Interactive Parameter Exploration
print("\nüß™ Interactive Parameter Exploration")

def explore_parameters(mu=1.0, epsilon=0.1, noise_level=0.05):
    """
    Explore how parameters affect limit cycle behavior.
    """
    # Create model with new parameters
    test_model = LimitCycleModel(mu=mu, epsilon=epsilon)
    
    # Generate data with specified noise
    test_data = data_gen.generate_limit_cycle(
        duration=60,  # 1 minute
        noise_level=noise_level,
        anomalies=False
    )
    
    # Process data
    test_monitor = StabilityMonitor(model=test_model)
    test_results = []
    
    for i in range(min(1000, len(test_data))):
        pitch, bank, power = test_data[i]
        result = test_monitor.process_sample(
            timestamp=i/sampling_rate,
            pitch=float(pitch),
            bank=float(bank),
            power=float(power)
        )
        test_results.append(result)
    
    test_df = pd.DataFrame(test_results)
    
    # Create visualization
    fig, axes = plt.subplots(2, 2, figsize=(12, 8))
    
    # Plot 1: Phase space (first 2 dimensions)
    axes[0, 0].plot(test_data[:200, 0], test_data[:200, 1], 'b-', alpha=0.7)
    axes[0, 0].set_xlabel('Pitch')
    axes[0, 0].set_ylabel('Bank')
    axes[0, 0].set_title('Phase Space Trajectory')
    axes[0, 0].grid(True, alpha=0.3)
    
    # Plot 2: Lyapunov exponent
    axes[0, 1].plot(test_df['lambda'], 'purple', alpha=0.7)
    axes[0, 1].axhline(y=0.01, color='orange', linestyle='--', alpha=0.5)
    axes[0, 1].set_xlabel('Sample')
    axes[0, 1].set_ylabel('Œª')
    axes[0, 1].set_title('Lyapunov Exponent')
    axes[0, 1].grid(True, alpha=0.3)
    
    # Plot 3: State distribution
    state_counts = test_df['state'].value_counts()
    axes[1, 0].bar(state_counts.index, state_counts.values, color=['green', 'orange', 'red'])
    axes[1, 0].set_xlabel('State')
    axes[1, 0].set_ylabel('Count')
    axes[1, 0].set_title('State Distribution')
    axes[1, 0].grid(True, alpha=0.3)
    
    # Plot 4: Parameters info
    axes[1, 1].axis('off')
    info_text = f"Parameters:\nŒº = {mu}\nŒµ = {epsilon}\nNoise = {noise_level}\n\n"
    info_text += f"Results:\nAvg Œª = {test_df['lambda'].mean():.3f}\n"
    info_text += f"CCZ Events = {sum(test_df['ccz_probability'] > 0.5)}\n"
    info_text += f"Stable % = {100*sum(test_df['state'] == 'STABLE')/len(test_df):.1f}%"
    axes[1, 1].text(0.1, 0.5, info_text, fontsize=12, verticalalignment='center')
    
    plt.suptitle(f"Parameter Exploration: Œº={mu}, Œµ={epsilon}, Noise={noise_level}", fontsize=14)
    plt.tight_layout()
    plt.show()
    
    return test_df

# Try different parameters
print("Example 1: Default parameters")
df1 = explore_parameters(mu=1.0, epsilon=0.1, noise_level=0.05)

print("\nExample 2: High nonlinearity")
df2 = explore_parameters(mu=3.0, epsilon=0.1, noise_level=0.05)

print("\nExample 3: High damping")
df3 = explore_parameters(mu=1.0, epsilon=0.5, noise_level=0.05)

print("\nExample 4: Noisy data")
df4 = explore_parameters(mu=1.0, epsilon=0.1, noise_level=0.2)
