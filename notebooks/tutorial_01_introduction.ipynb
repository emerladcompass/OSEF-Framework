# Cell 10: Analyze CCZ Events
print("\nüéØ Analyzing Creative Chaos Zone (CCZ) events...")

# Find CCZ events (probability > 50%)
ccz_events = results_df[results_df['ccz_probability'] > 0.5]

if len(ccz_events) > 0:
    print(f"‚úÖ Found {len(ccz_events)} CCZ events!")
    
    # Analyze first few events
    print("\nüìã First 3 CCZ Events:")
    for i, (idx, event) in enumerate(ccz_events.head(3).iterrows()):
        print(f"\nEvent {i+1}:")
        print(f"  Time: {idx/sampling_rate:.1f} seconds")
        print(f"  CCZ Probability: {event['ccz_probability']:.1%}")
        print(f"  Lyapunov Exponent: {event['lambda']:.3f}")
        print(f"  Recommendation: {event['recommendation']}")
        
    # Plot CCZ events on timeline
    fig, ax = plt.subplots(figsize=(12, 4))
    
    # Plot CCZ probability
    ax.plot(time_monitor, results_df['ccz_probability'], 
            'orange', alpha=0.7, linewidth=1, label='CCZ Probability')
    
    # Mark CCZ events
    ccz_times = ccz_events.index / sampling_rate
    ax.scatter(ccz_times, ccz_events['ccz_probability'], 
               color='red', s=50, zorder=5, label='CCZ Events')
    
    ax.axhline(y=0.5, color='red', linestyle='--', alpha=0.5, label='Threshold')
    ax.set_xlabel('Time (seconds)')
    ax.set_ylabel('CCZ Probability')
    ax.set_title('CCZ Event Detection Timeline')
    ax.legend()
    ax.grid(True, alpha=0.3)
    ax.set_ylim([0, 1])
    
    plt.tight_layout()
    plt.show()
    
else:
    print("‚ö†Ô∏è No CCZ events detected in this sample.")
    print("   This is actually good - it means the flight was stable!")
    print("   Try running with anomaly injection for CCZ examples.")
```

