```python
# Cell 5: Analyze CCZ Detection Performance
print("\nüìä Analyzing CCZ detection performance...")

# Calculate detection metrics
def calculate_detection_metrics(predictions, truths, threshold=0.5):
    """Calculate detection performance metrics."""
    
    pred_labels = predictions > threshold
    
    # Basic metrics
    tp = np.sum((pred_labels == True) & (truths == True))  # True positives
    tn = np.sum((pred_labels == False) & (truths == False))  # True negatives
    fp = np.sum((pred_labels == True) & (truths == False))  # False positives
    fn = np.sum((pred_labels == False) & (truths == True))  # False negatives
    
    # Calculate metrics
    accuracy = (tp + tn) / (tp + tn + fp + fn) if (tp + tn + fp + fn) > 0 else 0
    precision = tp / (tp + fp) if (tp + fp) > 0 else 0
    recall = tp / (tp + fn) if (tp + fn) > 0 else 0
    f1_score = 2 * precision * recall / (precision + recall) if (precision + recall) > 0 else 0
    
    # False alarm rate (per hour)
    false_alarm_rate = fp / len(truths) * 125 * 3600  # Convert to per hour
    
    # Detection latency (for true positives)
    detection_latencies = []
    if tp > 0:
        # Find when each CCZ event was first detected
        for ccz_time, duration in zip(ccz_times, ccz_durations):
            if ccz_time * 125 < len(predictions):
                start_idx = int(ccz_time * 125)
                end_idx = start_idx + int(duration * 125)
                
            

```python
                # Find first detection within CCZ period
                cc_predictions = predictions[start_idx:end_idx]
                if np.any(cc_predictions > threshold):
                    first_detection_idx = np.argmax(cc_predictions > threshold)
                    latency = first_detection_idx / 125  # Convert to seconds
                    detection_latencies.append(latency)
    
    avg_latency = np.mean(detection_latencies) if detection_latencies else 0
    
    return {
        'true_positives': tp,
        'true_negatives': tn,
        'false_positives': fp,
        'false_negatives': fn,
        'accuracy': accuracy,
        'precision': precision,
        'recall': recall,
        'f1_score': f1_score,
        'false_alarm_rate_per_hour': false_alarm_rate,
        'avg_detection_latency_s': avg_latency
    }

# Calculate metrics for our detection
metrics = calculate_detection_metrics(
    predictions=results_df['ccz_probability'].values,
    truths=results_df['true_ccz'].values,
    threshold=0.5
)

print("üìà CCZ Detection Performance Metrics:")
print(f"   - Accuracy: {metrics['accuracy']:.3f} ({metrics['accuracy']*100:.1f}%)")
print(f"   - Precision: {metrics['precision']:.3f}")
print(f"   - Recall: {metrics['recall']:.3f}")
print(f"   - F1 Score: {metrics['f1_score']:.3f}")
print(f"   - True Positives: {metrics['true_positives']}")
print(f"   - False Positives: {metrics['false_positives']}")
print(f"   - False Negatives: {metrics['false_negatives']}")
print(f"   - False Alarm Rate: {metrics['false_alarm_rate_per_hour']:.1f} per hour")
print(f"   - Avg Detection Latency: {metrics['avg_detection_latency_s']:.2f} seconds")

