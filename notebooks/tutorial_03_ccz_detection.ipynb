# Cell 4: Real-Time CCZ Detection Simulation
print("\n‚ö° Simulating real-time CCZ detection...")

# We'll process the first 15000 samples (2 minutes at 125 Hz)
n_samples = min(15000, len(flight_data_with_ccz))
detection_results = []

print(f"Processing {n_samples:,} samples ({n_samples/125:.1f} seconds)...")

for i in range(n_samples):
    # Get current sample
    pitch, bank, power = flight_data_with_ccz[i]
    timestamp = i / 125  # Current time in seconds
    
    # Process through monitor
    result = monitor.process_sample(
        timestamp=timestamp,
        pitch=float(pitch),
        bank=float(bank),
        power=float(power)
    )
    
    # Add ground truth label
    result['true_ccz'] = bool(true_ccz_labels[i])
    result['sample_index'] = i
    
    detection_results.append(result)
    
    # Show progress
    if i % 3000 == 0 and i > 0:
        ccz_detected = sum(1 for r in detection_results[-3000:] if r['ccz_probability'] > 0.5)
        print(f"  Processed {i:,} samples... ({ccz_detected} CCZ alerts in last 24s)")

print(f"\n‚úÖ Real-time processing complete!")
print(f"   - Total samples processed: {len(detection_results):,}")
print(f"   - Processing rate: {n_samples/(len(detection_results)*0.001):.0f} Hz")

# Convert to DataFrame for analysis
results_df = pd.DataFrame(detection_results)
