# Cell 2: Generate Flight Data with CCZ Events
print("\n‚úàÔ∏è Generating flight data with embedded CCZ events...")

# Create data generator with CCZ injection capabilities
data_gen = SyntheticDataGenerator()

# Generate baseline flight data (stable flight)
print("  Generating baseline stable flight...")
stable_data = data_gen.generate_limit_cycle(
    duration=300,           # 5 minutes
    noise_level=0.03,
    anomalies=False
)

# Generate CCZ events at specific times
print("  Injecting CCZ events...")
ccz_times = [60, 120, 180, 240]  # CCZ events at 1, 2, 3, 4 minutes
ccz_durations = [5, 8, 6, 7]     # CCZ durations in seconds
ccz_severities = [0.3, 0.5, 0.4, 0.6]  # Severity (0-1)

# Inject CCZ events
flight_data_with_ccz = stable_data.copy()
true_ccz_labels = np.zeros(len(flight_data_with_ccz), dtype=bool)

for ccz_time, duration, severity in zip(ccz_times, ccz_durations, ccz_severities):
    print(f"    Injecting CCZ at {ccz_time}s for {duration}s (severity: {severity})...")
    
    # Convert time to sample indices
    start_idx = int(ccz_time * 125)
    end_idx = start_idx + int(duration * 125)
    
    # Generate CCZ pattern
    ccz_pattern = data_gen.generate_ccz_pattern(
        duration=duration,
        severity=severity,
        mode='oscillatory'  # Oscillatory instability
    )
    
    # Inject into flight data
    if end_idx <= len(flight_data_with_ccz):
        flight_data_with_ccz[start_idx:end_idx] = ccz_pattern
        true_ccz_labels[start_idx:end_idx] = True

print(f"\n‚úÖ Flight data generated with {len(ccz_times)} CCZ events")
print(f"   - Total samples: {len(flight_data_with_ccz):,}")
print(f"   - CCZ samples: {true_ccz_labels.sum():,} ({100*true_ccz_labels.sum()/len(flight_data_with_ccz):.1f}%)")
print(f"   - CCZ event locations: {ccz_times} seconds")
