class CCZMonitoringSystem:
    def __init__(self, alert_threshold=0.5, warning_threshold=0.3):
        self.alert_threshold = alert_threshold
        self.warning_threshold = warning_threshold
        self.alert_history = []
        self.current_status = "NORMAL"
        self.ccz_duration = 0
        self.max_ccz_duration = 0
        
    def update_status(self, lambda_val, probability, timestamp):
        """Update system status based on new readings"""
        status = "NORMAL"
        alert_level = 0  # 0: normal, 1: warning, 2: alert
        
        if probability > self.alert_threshold:
            status = "CCZ ALERT üö®"
            alert_level = 2
            self.ccz_duration += 1/125  # Add 1 sample duration
            if self.ccz_duration > self.max_ccz_duration:
                self.max_ccz_duration = self.ccz_duration
        elif probability > self.warning_threshold:
            status = "CCZ WARNING ‚ö†Ô∏è"
            alert_level = 1
            self.ccz_duration = 0
        else:
            status = "NORMAL ‚úÖ"
            alert_level = 0
            self.ccz_duration = 0
            
        # Record alert if level changed
        if alert_level > 0 and (not self.alert_history or self.alert_history[-1]['level'] != alert_level):
            alert_record = {
                'timestamp': timestamp,
                'level': alert_level,
                'lambda': lambda_val,
                'probability': probability,
                'status': status
            }
            self.alert_history.append(alert_record)
            
        self.current_status = status
        return status, alert_level
    
    def get_statistics(self):
        """Get monitoring statistics"""
        stats = {
            'total_alerts': len([a for a in self.alert_history if a['level'] == 2]),
            'total_warnings': len([a for a in self.alert_history if a['level'] == 1]),
            'max_ccz_duration': self.max_ccz_duration,
            'current_status': self.current_status
        }
        return stats
    
